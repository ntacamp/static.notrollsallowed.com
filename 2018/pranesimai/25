          <div class="panel-heading">
            <h1>Offline-first WEB</h1>
                                  </div>

          <!-- Table -->
          <table class="table">
              <tr>
                  <td><strong>Speaker</strong></td>
                  <td>
                                        Aurelijus Banelis
                                        &nbsp;
                                                                <a href="http://github.com/https://github.com/aurelijusb"><img src="/2018/images/icon_github.png?v3" alt="https://github.com/aurelijusb"></a>
                                                                <a href="http://facebook.com/aurelijus.banelis"><img src="/2018/images/icon_facebook.png?v3" alt=""></a>
                                                                                    <a href="http://lnkd.in/iietfc"><img src="/2018/images/icon_linkedin.png?v3" alt=""></a>
                                                                                    <a href="http://aurelijus.banelis.lt/"><img src="/2018/images/icon_homepage.png?v3" alt=""></a>
                                      </td>
              </tr>
